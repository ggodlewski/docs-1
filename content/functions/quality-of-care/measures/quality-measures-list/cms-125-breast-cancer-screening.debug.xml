xt:list-item><text:list-item><text:p text:style-name="P4">Taking dementia medications during the measurement period or the year prior</text:p></text:list-item></text:list></text:list-item><text:list-item><text:p text:style-name="P2">Exclude patients receiving palliative care during the measurement period.</text:p></text:list-item></text:list></table:table-cell></table:table-row><table:table-row table:style-name="Table2.1"><table:table-cell table:style-name="Table2.A1" office:value-type="string"><text:p text:style-name="Standard"><text:span text:style-name="T2">Numerator</text:span></text:p></table:table-cell><table:table-cell table:style-name="Table2.A1" office:value-type="string"><text:p text:style-name="Standard">Women with one or more mammograms during the 27 months prior to the end of the measurement period</text:p></table:table-cell></table:table-row><table:table-row table:style-name="Table2.1"><table:table-cell table:style-name="Table2.A1" office:value-type="string"><text:p text:style-name="Standard"><text:span text:style-name="T2">Numerator Exclusions</text:span></text:p></table:table-cell><table:table-cell table:style-name="Table2.A1" office:value-type="string"><text:p text:style-name="Standard">–</text:p></table:table-cell></table:table-row><table:table-row table:style-name="Table2.1"><table:table-cell table:style-name="Table2.A1" office:value-type="string"><text:p text:style-name="Standard"><text:span text:style-name="T2">Denominator Exceptions</text:span></text:p></table:table-cell><table:table-cell table:style-name="Table2.A1" office:value-type="string"><text:p text:style-name="Standard">–</text:p></table:table-cell></table:table-row></table:table><text:p text:style-name="P1"/><text:p text:style-name="Heading_20_3"><text:bookmark text:name="_dij01uvl9gm5"/>Additional Information</text:p><table:table table:name="Table3" table:style-name="Table3"><table:table-column table:style-name="Table3.A"/><table:table-column table:style-name="Table3.B"/><text:soft-pa